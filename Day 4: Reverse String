//Input: ["h","e","l","l","o"]
//Output: ["o","l","l","e","h"]


class Solution {
    private static final Scanner scanner = new Scanner(System.in);
    public void reverseString(char[] s) {
        
        int start=0;
        int end=s.length-1;
        while(start<end){
            char temp = s[start];
            s[start] = s[end];
            s[end] = temp;
            
            start++;
            end--;
        }
    }
}
